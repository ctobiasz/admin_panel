  <body>

    <%= render 'layouts/sidenav' %>

<div class="flash_success">
  <%= flash[:notice] %>
</div>

<div class="alert-lg" role="alert" id="alert" style="">
  Deletion Complete
</div>


    <div class="main-wrapper">
    <%= yield %>
