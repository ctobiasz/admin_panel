<h2>All Instructors</h2>

<table>
  <tr>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Age</th>
  <th>Salary</th>
  <th>Education</th>

  </tr>
<% @instructor.each do |instu| %>
    <tr>
      <td><%= instu.first_name %></td>
      <td><%= instu.last_name %></td>
      <td><%= instu.age %></td>
      <td><%= instu.salary %></td>
      <td><%= instu.education %></td>
      <td><a href="/instructors/<%= instu.id %>">View Instructor</a></td>
    </tr>
    <% end %>
  </table>

<%= link_to "Go back", root_path%>
