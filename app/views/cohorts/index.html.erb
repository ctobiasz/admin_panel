<h2>Cohorts</h2>

<table>
  <tr>
  <th>Cohort Name</th>
  <th>Start Date</th>
  <th>End Date</th>

  </tr>
<% @cohort.each do |cohort| %>
    <tr>
      <td><%= cohort.name %></td>
      <td><%= cohort.start_date %></td>
      <td><%= cohort.end_date %></td>

      <td><a href="/cohorts/<%= cohort.id %>">View Cohort</a></td>
    </tr>
    <% end %>
</table>

<br>

<%= link_to "Go back", root_path %>
